<div class="row">
  <div class="col-md-9">
    <%= link_to t(".link_category"), categories_path, class: "category-head" %>
  </div>
  <div class="col-md-3">
    <%= render "search_form" %>
  </div>
</div>
<div class="row">
  <div class="col-md-4"></div>
  <div class="col-md-8">
    <div class="categories-content">
      <%= render @categories %>
    </div>
    <%= will_paginate @categories %>
  </div>
</div>
